<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SB_Preferred_Billing_County__c</fullName>
    <description>This is the preferred billing county (from tax county field). If no preferred billing address is selected, Work/School is used.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(FS_Preferred_Billing_Address__c, "Home"),  FS_Other_County__c,  FS_Mailing_County__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Billing Tax County</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
