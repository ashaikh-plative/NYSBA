<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SB_Preferred_Mailing_Zip__c</fullName>
    <description>This is the Preferred Mailing Zip/postal code. If no Preferred Mailing Address is select, Work/School is used.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(FS_Preferred_Mailing_Address__c, "Home"),   OtherPostalCode,  MailingPostalCode)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Mailing Zip</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
