<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SB_Preferred_Mailing_Tax_County__c</fullName>
    <description>This is the Preferred Mailing County (based on tax county field). If no Preferred Mailing Address is select, Work/School is used.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(FS_Preferred_Mailing_Address__c, "Home"),   FS_Other_County__c,  FS_Mailing_County__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Mailing Tax County</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
