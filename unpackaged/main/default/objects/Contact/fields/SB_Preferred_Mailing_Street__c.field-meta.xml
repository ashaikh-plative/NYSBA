<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SB_Preferred_Mailing_Street__c</fullName>
    <description>This is the preferred mailing street. If no preferred mailing address has been selected, Work/School is used.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(FS_Preferred_Mailing_Address__c, "Home"),   OtherStreet,  MailingStreet)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Mailing Street</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
