<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SB_Preferred_Billing_City__c</fullName>
    <description>This is the preferred billing City. If no preferred billing address has been selected, Work/School is used.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(FS_Preferred_Billing_Address__c, "Home"),  OtherCity,  MailingCity)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Billing City</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
