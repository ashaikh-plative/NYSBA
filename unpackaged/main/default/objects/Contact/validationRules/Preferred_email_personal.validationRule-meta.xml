<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Preferred_email_personal</fullName>
    <active>false</active>
    <description>Validation rule to ensure preferred email is not blank</description>
    <errorConditionFormula>ISPICKVAL(OrderApi__Preferred_Email_Type__c,"Personal") &amp;&amp; ISBLANK(  OrderApi__Personal_Email__c )</errorConditionFormula>
    <errorDisplayField>OrderApi__Personal_Email__c</errorDisplayField>
    <errorMessage>Preferred Email cannot be blank</errorMessage>
</ValidationRule>
